{% extends 'base.html.twig' %}
{% block body %}
    <div class="blog-header">
        <h1 class="blog-title">{{ settings.title }}</h1>
    </div>
    <div class="row">
        <div class="col-sm-8 blog-main">
            <div class="blog-post">
                <h2 class="blog-post-title">{{ settings.title }}</h2>
                <p{{ settings.body }}</p>
            </div><!-- /.blog-post -->
    </div><!-- /.blog-main -->
{% endblock body %}
